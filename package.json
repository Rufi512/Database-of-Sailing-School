{
  "name": "database-of-sailing-school",
  "version": "1.0.0",
  "description": "Stack MERN application to register, consult, modify and delete student grades",
  "main": "index.js",
  "scripts": {
    "all-dev": "concurrently \"npm run dev\" \"npm run client\"",
    "prebuild": "rimraf ./build",
    "build": "babel src --ignore src/client --out-dir build && cd src/client && npm run build",
    "start": "node build/index.js",
    "client": "cd src/client && npm run start",
    "dev": "nodemon src/index.js --exec babel-node"
  },
  "dependencies": {
    "awesome-phonenumber": "^3.0.2",
    "bcrypt": "^5.0.1",
    "core-js": "^3.24.0",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "dotenv": "16.0.1",
    "ejs": "^3.1.8",
    "express": "4.18.1",
    "fast-csv": "^4.3.6",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "6.3.4",
    "mongoose-paginate-v2": "1.6.3",
    "morgan": "^1.10.0",
    "multer": "1.4.4-lts.1",
    "nodemailer": "^6.7.5",
    "read-excel-file": "^5.3.3",
    "recaptcha2": "1.3.3",
    "regenerator-runtime": "^0.13.9",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2"
  },
  "keywords": [],
  "author": "Rufi512",
  "license": "ISC"
}
